{
  "schema": "aieip_spec_schema-0.1",
  "ip_name": "vmc_ifft",
  "block_info": {
    "label": "IFFT",
    "description": "Inverse FFT implementation block for AI Engines."
  },
  "parameters": [
    {
      "name": "data_type",
      "label": "Input/Output data type",
      "description": "The type of individual data samples input to and output of the IFFT function",
      "enum": [ "cint16", "cint32", "cfloat" ],
      "default": "cint16",
      "type": "typename",
      "blockgui":{
            "tablabel": "Main",
            "parameterType":"popup"
      }
    },
    {
      "name": "point_size",
      "label": "IFFT size",
      "description": "The number of samples in the transform",
      "default": 64,
      "maximum": 4096,
      "minimum": 16,
      "type": "int",
      "validator": {
        "file": "vmc_ifft.py",
        "function": "vmc_validate_point_size",
        "args": ["data_type"]
      },
      "blockgui":{
            "tablabel": "Main",
            "parameterType":"edit"
      }
    }, 
    {
      "name": "input_window_size",
      "label": "Input window size (Number of Samples)",
      "description": "The number of samples in the input window",
      "default": 64,
      "maximum": 4096,
      "minimum": 16,
      "type": "int",
      "validator": {
        "file": "vmc_ifft.py",
        "function": "vmc_validate_input_window_size",
        "args": ["point_size"]
      },
      "blockgui":{
            "tablabel": "Main",
            "parameterType":"edit"
      }
    },
    {
      "name": "shift_val",
      "label": "Scale output down by 2^",
      "description": "The power of 2 shift down applied to the accumulation of FIR before output",
      "default": 0,
      "maximum": 60,
      "minimum": 0,
      "type": "int",
      "validator": {
        "file": "vmc_ifft.py",
        "function": "vmc_validate_shift_val",
        "args": ["data_type", "point_size"]
      },
      "blockgui":{
            "tablabel": "Main",
            "parameterType":"edit"
      }
    },
    {
      "name": "casc_length",
      "label": "Number of cascade stages",
      "description": "The length of cascaded kernels",
      "type": "int",
      "default": 1,
      "maximum": 11,
      "minimum": 1,
      "validator": {
        "file": "vmc_ifft.py",
        "function": "vmc_validate_casc_length",
        "args": ["data_type", "point_size"]
      },
      "blockgui":{
            "tablabel": "Main",
            "parameterType":"edit"
      }
    }
  ],
  "generator": {
    "file": "vmc_ifft.py",
    "function": "vmc_generate_graph"
  }
}
