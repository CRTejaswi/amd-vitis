			"SDKTARGETSYSROOT=${SYSROOT}",
			"ROOTFS=${EDGE_COMMON_SW}/rootfs.ext4",
			"IMAGE=${EDGE_COMMON_SW}/Image",
            "EMU_CMD=\\\"./launch_hw_emu.sh -run-app embedded_exec.sh\\\""
        ],
        "custom_build_target":
        {
            "all": "all_run_emu"
        },
        "tasks": {
            "build": {
                "pre_exec": "./env_setup_versal.sh"
