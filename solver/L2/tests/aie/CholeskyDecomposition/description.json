{
    "name": "Xilinx Cholesky Decomposition Test",
    "description": "Cholesky Decomposition on AIE",
    "flow": "versal",
    "platform_allowlist": [
        "vck190"
    ],
    "data": [
        "PROJECT/data"
    ],
    "platform_blocklist": [],
    "platform_properties": {
        "vck190": {
            "v++": {
                "compiler": {
                    "cflags": [
                        ""
                    ]
                },
                "linker": {
                    "cflags": [
                        "--config PROJECT/system.cfg"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "name": "hwemu-launch",
            "target": "hw_emu",
            "pkg-dir": "$(WORK_DIR)",
            "input-dir": "./",
            "output-dir": "./"
        },
        {
            "name": "aiesim-launch",
            "target": "aiesim",
            "pkg-dir": "$(WORK_DIR)",
            "sim_options": "--input-dir=PROJECT --output-dir=PROJECT",
            "input-dir": "./",
            "output-dir": "./"
        },
        {
            "name": "x86-launch",
            "target": "x86sim",
            "pkg-dir": "$(WORK_DIR)",
            "sim_options": "--input-dir=PROJECT --output-dir=PROJECT",
            "input-dir": "./",
            "output-dir": "./"
        }
    ],
    "host": {
        "host_exe": "host.elf",
        "compiler": {
            "sources": [
                "PROJECT/ps/host.cpp",
                "./Work/ps/c_rts/aie_control_xrt.cpp"
            ],
            "includepaths": [
                "$(SYSROOT)/usr/include/xrt/",
                "./"
            ],
            "options": "--sysroot=$(SYSROOT)"
        },
        "linker": {
            "librarypaths": [
                "$(SYSROOT)/usr/lib/"
            ],
            "options": "--sysroot=$(SYSROOT)"
        }
    },
    "v++": {
        "compiler": {
            "includepaths": []
        }
    },
    "aiecompiler": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L1/include",
                "LIB_DIR/L2/include",
                "LIB_DIR/L1/src"
            ]
        }
    },
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "PROJECT/aie/platform.cpp",
                    "files": ["LIB_DIR/L1/src/aie/cholesky_decomp_kernels.cpp"],
                    "pl-freq": 250,
                    "workdir": "./Work",
                    "name": "libadf.a"
                }
            ]
        }
    ],
    "containers": [
        {
            "accelerators": [
                {
                    "location": "PROJECT/pl/pl_mm2s.cpp",
                    "frequency": 250.0,
                    "clflags": " ",
                    "name": "data_mover"
                }
            ],
            "name": "kernel"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": {
                    "vitis_aie_sim": 55,
                    "vitis_aie_x86sim": 55,
                    "vitis_hw_build": 470,
                    "vitis_hw_emu": 200,
                    "vitis_hw_run": 30
                }
            }
        ],
        "targets": [
            "vitis_aie_sim",
            "vitis_aie_x86sim"
        ],
        "category": "canary"
    }
}
