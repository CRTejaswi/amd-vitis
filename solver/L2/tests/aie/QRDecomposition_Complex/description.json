{
    "name": "AIE Adder", 
    "description": [
        "This simple example demonstrate the flow to create aie Kernel and how to connect aie Kernel to PL Kernels and validate the design running on the AI Engine."
    ],
    "flow": "versal",
    "keywords": [
        "aiecompiler", 
        "aiesimulator",
        "stream_connect"
    ], 
    "key_concepts": [
        "AI Engine", 
        "Adaptive Data Flow", 
        "Graph"
    ],
    "platform_whitelist": [
        "vck190"
    ],
    "platform_blacklist": [

    ],
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "host": {
       "host_exe": "aie_adder",
        "compiler": {
            "sources": [
                "./ps/host.cpp",
                "./Work/ps/c_rts/aie_control_xrt.cpp"
            ], 
            "includepaths": [
                "./",
                "LIB_DIR/L1/include",
                "$(SYSROOT)/usr/include/xrt/"                
            ],
            "options": "--sysroot=$(SYSROOT)"
        }
    },
    "aiecompiler": {
        "compiler": {
            "includepaths": [
                "./aie"
            ]
        }
    },
    "aiecontainers": [
        {
            "accelerators": [
                {
                    "location": "./aie/aie_graph.cpp",
                    "pl-freq": 1000,
                    "workdir": "./Work",
                    "name": "libadf.a",
                    "aie_kernel_files":["./aie/aie_adder.cc"]
                }
            ]
        }
    ],
    "data": [
        "PROJECT/data/golden.txt",
        "PROJECT/data/input0.txt",
        "PROJECT/data/input1.txt"
    ],
    "v++": {
        "linker": {
                    "cflags": [
                        "--config PROJECT/system.cfg"
                    ]
                },
        "package": {
                    "cflags": [
                        "--package.defer_aie_run"
                    ]
                }
     },
    "match_makefile" : "true",
    "containers": [
        {
            "accelerators": [
                {
                    "name": "pl_mm2s", 
                    "location": "pl_kernels/pl_mm2s.cpp",
                    "num_compute_units": "1"
                },
                {
                    "name": "pl_s2mm", 
                    "location": "pl_kernels/pl_s2mm.cpp",
                    "num_compute_units": "1"
                }
            ], 
            "name": "krnl_adder"
        }
    ],
    "launch": [
        {
            "cmd_args": "BUILD/krnl_adder.xclbin", 
            "name": "generic launch for all flows"
        }
    ],
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_aie_sim",
            "vitis_aie_x86sim"
        ],
        "category": "canary"
    }
}
